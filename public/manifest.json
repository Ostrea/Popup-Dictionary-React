{
  "manifest_version": 2,

  "name": "Popup Dictionary",
  "description": "This extension shows a word translation from OED of the currently selected word.",
  "version": "1.0",
  
  "browser_action": {
      "default_popup": "index.html",
      "default_title": "Click here!"
  },
  "permissions": [
      "<all_urls>"
  ],
  "icons": {
      "48": "icon.png",
      "128": "icon.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'"
}
